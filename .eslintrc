{
  "env": {
    "node": true,
    "browser": true
  },
  "rules": {
    "curly": [2, "multi-line"],
    "consistent-return": 0,
    "quotes": [2, "single"],
    "semi": [2, "never"],
    "strict": 0,
    "no-underscore-dangle": 0,
    "no-use-before-define": [2, "nofunc"],
    "no-unused-vars": [2, { "vars": "all", "args": "none" }]
  }
}
